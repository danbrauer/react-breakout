{
  "name": "react-tic-tac-toe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "tsc --build && react-scripts start & tsc --build --watch",
    "build": "react-scripts build",
    "test": "jest -c jest.config.json --verbose",
    "eject": "react-scripts eject",
    "prettier:write": "prettier --write \"{src,__test__}/**/*.{js,jsx,ts,tsx}\""
